{
  "name": "donationweb-3.0",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register test/routes/donations-test.js",
    "postbuild": "npm run server",
    "prebuild": "npm-run-all test clean",
    "build": "babel ./ --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./test --copy-files",
    "server": "cross-env NODE_ENV=dev PORT=3001 node lib/bin/www",
    "start": "npm-run-all test build server",
    "clean": "rimraf ./lib && mkdir lib",
    "server:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "test:watch": "npm run test -- --watch",
    "lint": "esw  ./test ./routes ./models/ app.js",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "angular": "^1.3.10",
    "angular-route": "^1.3.10",
    "body-parser": "~1.12.4",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.4",
    "debug": "~2.2.0",
    "ejs": "~2.3.1",
    "express": "~4.12.4",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "morgan": "~1.5.3",
    "serve-favicon": "~2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "chai-things": "^0.2.0",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.2.0",
    "css-loader": "^0.24.0",
    "eslint": "^4.1.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-watch": "^4.0.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.30.1",
    "istanbul": "^0.4.5",
    "mocha": "^2.4.5",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^1.15.0"
  }
}
